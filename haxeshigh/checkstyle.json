{
	"defaultSeverity": "INFO",
	"extendsConfigPath": "",
	"numberOfCheckerThreads": 5,
	"baseDefines": [],
	"defineCombinations": [],
	"checks": [
		{
			"type": "Anonymous",
			"props": {}
		},
		{
			"type": "AvoidStarImport",
		  "props": {
            "severity": "WARNING"
            }
		},
		{
			"type": "BlockBreakingConditional",
			"props": {}
		},
		{
			"type": "CodeSimilarity",
			"props": {
				"severityIdentical": "WARNING",
				"thresholdIdentical": 60,
				"thresholdSimilar": 120
			}
		},
		{
			"type": "ConstantName",
			"props": {
				"format": "^[a-z][a-zA-Z0-9]*$",
				"tokens": [
					"NOTINLINE"
				],
				"ignoreExtern": false
			}
		},
		{
			"type": "Dynamic",
			"props": {}
		},
		{
			"type": "EmptyLines",
			"props": {
				"max": 2,
				"allowEmptyLineAfterSingleLineComment": true,
				"allowEmptyLineAfterMultiLineComment": true,
				"requireEmptyLineAfterPackage": true,
				"requireEmptyLineAfterClass": false,
				"requireEmptyLineAfterInterface": false,
				"requireEmptyLineAfterAbstract": false
			}
		},
		{
			"type": "ExtendedEmptyLines",
			"props": {
				"max": 3,
				"skipSingleLineTypes": true,
				"defaultPolicy": "ignore",
				"ignore": [],
				"none": [
					"beforePackage",
					"afterLeftCurly",
					"beforeRightCurly"
				],
				"exact": [],
				"upto": [],
				"atleast": []
			}
		},
		{
			"type": "Final",
		  "props": {"severity": "INFO"}
		},
		{
			"type": "Indentation",
			"props": {
				"character": "  ",
				"ignoreConditionals": false,
				"ignoreComments": false,
				"wrapPolicy": "larger",
				"conditionalPolicy": "ignore"
			}
		},
		{
			"type": "IndentationCharacter",
			"props": {
				"character": "space",
				"ignorePattern": "^$"
			}
		},
		{
			"type": "LeftCurly",
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"INTERFACE_DEF",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "eol",
				"ignoreEmptySingleline": true,
				"ignoreSingleline": true
			}
		},
		{
			"type": "LineLength",
			"props": {
				"max": 100,
				"ignorePattern": "^$"
			}
		},
		{
			"type": "MethodCount",
			"props": {
				"maxTotal": 15,
				"maxPrivate": 110,
				"maxPublic": 110
			}
		},
		{
			"type": "MethodLength",
			"props": {
			  "max": 20,
              "severity": "WARNING",
				"ignoreEmptyLines": false
			}
		},
		{
			"type": "OperatorWhitespace",
			"props": {
				"assignOpPolicy": "around",
				"unaryOpPolicy": "ignore",
				"ternaryOpPolicy": "ignore",
				"arithmeticOpPolicy": "around",
				"compareOpPolicy": "around",
				"bitwiseOpPolicy": "ignore",
				"boolOpPolicy": "ignore",
				"intervalOpPolicy": "none",
				"arrowPolicy": "ignore",
				"arrowFunctionPolicy": "around",
				"oldFunctionTypePolicy": "ignore",
				"newFunctionTypePolicy": "around"
			}
		},
		{
			"type": "RedundantAccessMeta",
			"props": {
				"prohibitMeta": false
			}
		},
		{
			"type": "RedundantAllowMeta",
			"props": {
				"prohibitMeta": false
			}
		},
		{
			"type": "RedundantModifier",
			"props": {
				"enforcePublicPrivate": false,
				"enforcePublic": false,
				"enforcePrivate": false
			}
		},
		{
			"type": "Return",
			"props": {
                "severity": "WARNING",
				"allowEmptyReturn": false,
				"enforceReturnType": false,
				"enforceReturnTypeForAnonymous": false
			}
		},
		{
			"type": "RightCurly",
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"OBJECT_DECL",
					"FUNCTION",
					"FOR",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "aloneorsingle"
			}
		},
		{
			"type": "RightCurly",
			"props": {
				"tokens": [
					"IF"
				],
				"option": "aloneorsingle"
			}
		},
		{
			"type": "RightCurly",
			"props": {
				"tokens": [
					"OBJECT_DECL"
				],
				"option": "aloneorsingle"
			}
		},
		{
			"type": "SeparatorWhitespace",
			"props": {
				"dotPolicy": "none",
				"commaPolicy": "after",
				"semicolonPolicy": "after",
				"allowTrailingComma": false
			}
		},
		{
			"type": "SeparatorWrap",
			"props": {
				"option": "eol",
				"tokens": [
					","
				]
			}
		},
		{
			"type": "Spacing",
			"props": {
				"spaceIfCondition": "should",
				"spaceForLoop": "should",
				"spaceWhileLoop": "any",
				"spaceSwitchCase": "should",
				"spaceCatch": "any",
				"spaceAroundBinop": false,
				"noSpaceAroundUnop": false,
				"ignoreRangeOperator": true
			}
		},
		{
			"type": "StringLiteral",
			"props": {
				"policy": "doubleAndInterpolation",
				"allowException": true
			}
		},
		{
			"type": "TODOComment",
			"props": {
				"format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)"
			}
		},
		{
			"type": "Trace",
			"props": {}
		},
		{
			"type": "TrailingWhitespace",
			"props": {}
		},
		{
			"type": "Type",
		  "props": {
            "severity": "WARNING",
				"ignoreEnumAbstractValues": true
			}
		},
		{
			"type": "UnnecessaryConstructor",
			"props": {}
		},
		{
			"type": "UnusedImport",
			"props": {
				"ignoreModules": [],
			  "moduleTypeMap": {},
              "severity": "ERROR"
			}
		},
		{
			"type": "UnusedLocalVar",
		  "props": {

            "severity": "WARNING"
            }
		}
	],
	"exclude": {},
	"version": 1
}
